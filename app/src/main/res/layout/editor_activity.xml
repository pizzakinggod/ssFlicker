<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/rl_all"
	style="@style/lp_mp_mp" >
	<com.ssmomonga.ssflicker.view.DockWindow
		android:id="@+id/dock_window"
	    style="@style/lp_wc_wc"
		android:baselineAligned="false" />
	<com.ssmomonga.ssflicker.view.PointerWindow
		android:id="@+id/pointer_window"
		style="@style/lp_wc_wc"
		android:padding="4dp" />
	<com.ssmomonga.ssflicker.view.AppWindow
		android:id="@+id/app_window"
		style="@style/lp_wc_wc"
		android:padding="4dp"
		android:visibility="gone" />
	<com.ssmomonga.ssflicker.view.ActionWindow
		android:id="@+id/action_window"
		style="@style/lp_wc_wc"
		android:padding="4dp"
		android:visibility="gone" />
</RelativeLayout>